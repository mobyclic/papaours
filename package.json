{
  "name": "papaours",
  "version": "0.0.1",
  "scripts": {
    "dev": "sh -c 'fuser -k 5173/tcp 2>/dev/null || (lsof -ti:5173 | xargs -r kill -9) || true; exec bunx --bun vite dev'",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "start": "node build/index.js",
    "prepare": "bunx --bun svelte-kit sync || echo ''",
    "check": "bunx --bun svelte-kit sync && bunx --bun svelte-check --tsconfig ./tsconfig.json",
    "db:init": "bun run scripts/init-db.ts"
  },
  "type": "module",
  "devDependencies": {
    "@internationalized/date": "^3.10.1",
    "@lucide/svelte": "^0.561.0",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-node": "^5.5.2",
    "@sveltejs/kit": "^2.50.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bun": "^1.3.8",
    "@types/canvas-confetti": "^1.9.0",
    "autoprefixer": "^10.4.24",
    "bun-types": "^1.3.8",
    "postcss": "^8.5.6",
    "svelte": "^5.49.1",
    "svelte-check": "^4.3.6",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.980.0",
    "bits-ui": "^2.15.5",
    "canvas-confetti": "^1.9.4",
    "clsx": "^2.1.1",
    "lucide-svelte": "^0.563.0",
    "resend": "^6.9.1",
    "stripe": "^20.3.0",
    "surrealdb": "^1.3.2",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2"
  }
}
